        <mxCell id="oab-box" value="OAB DRUGS" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="60" y="810" width="220" height="40" as="geometry" />
