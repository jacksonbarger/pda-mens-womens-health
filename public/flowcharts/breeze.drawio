        <mxCell id="beta3" value="Beta-3 Agonists&#xa;• Mirabegron (Myrbetriq)&#xa;• Vibegron&#xa;&#xa;MOA: Relax detrusor muscle&#xa;&#xa;Advantages:&#xa;• NO anticholinergic SE&#xa;• Better tolerated" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="300" y="865" width="220" height="140" as="geometry" />
        </mxCell>

