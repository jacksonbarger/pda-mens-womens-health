        <!-- PDE5 Inhibitors -->
        <mxCell id="pde5-box" value="PDE5 INHIBITORS (Erectile Dysfunction)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#cfe2f3;strokeColor=#6c8ebf;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="60" y="210" width="220" height="40" as="geometry" />
        </mxCell>

        <mxCell id="sildenafil" value="Sildenafil (Viagra)&#xa;• Onset: 30-60 min, Duration: 4-6 hr&#xa;• Poor PDE5/PDE6 selectivity&#xa;• Blue vision side effect&#xa;• Take 1 hr before sex" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="60" y="265" width="220" height="95" as="geometry" />
        </mxCell>

        <mxCell id="tadalafil" value="Tadalafil (Cialis)&#xa;• Onset: 30 min, Duration: 24-36 hr&#xa;• LONGEST duration&#xa;• Daily or PRN dosing&#xa;• Better PDE selectivity" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="60" y="375" width="220" height="90" as="geometry" />
        </mxCell>

        <mxCell id="avanafil" value="Avanafil (Stendra)&#xa;• FASTEST onset: 15-30 min&#xa;• Duration: 6 hr&#xa;• Fewer side effects&#xa;• Most selective for PDE5" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="60" y="480" width="220" height="90" as="geometry" />
        </mxCell>

        <mxCell id="pde5-moa" value="MOA: Block PDE5 → ↑ cGMP&#xa;→ Smooth muscle relaxation&#xa;→ Vasodilation → Erection&#xa;&#xa;WARNING: ABSOLUTE CI with nitrates&#xa;= SEVERE HYPOTENSION" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;fontSize=11;align=left;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="60" y="585" width="220" height="110" as="geometry" />
        </mxCell>

        <!-- BPH Drugs -->
        <mxCell id="bph-box" value="BPH DRUGS" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#cfe2f3;strokeColor=#6c8ebf;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="310" y="210" width="210" height="40" as="geometry" />
        </mxCell>

        <mxCell id="alpha-blockers" value="Alpha-1 Blockers&#xa;• Tamsulosin (Flomax): SELECTIVE&#xa;  alpha-1A (prostate)&#xa;  → LESS orthostatic hypotension&#xa;• Alfuzosin, Silodosin&#xa;• Immediate symptom relief" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="310" y="265" width="210" height="120" as="geometry" />
        </mxCell>

        <mxCell id="5ari" value="5-Alpha-Reductase Inhibitors&#xa;• Finasteride: Type 2 only&#xa;• Dutasteride: Type 1 + 2&#xa;  (more complete DHT suppression)&#xa;• Takes 6+ MONTHS for effect&#xa;• DOUBLE PSA values for screening&#xa;• WARNING: Teratogenic" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="310" y="400" width="210" height="140" as="geometry" />
        </mxCell>

        <mxCell id="bph-combo" value="Combination Therapy:&#xa;Alpha blocker (immediate relief)&#xa;+ 5-ARI (long-term shrinkage)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;fontSize=11;align=left;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="555" width="210" height="70" as="geometry" />
        </mxCell>

