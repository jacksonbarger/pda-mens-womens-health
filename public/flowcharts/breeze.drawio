        <mxCell id="denosumab" value="Denosumab (Prolia)&#xa;• Monoclonal Ab against RANKL&#xa;• SC q6 months&#xa;&#xa;WARNING: Rapid bone loss if&#xa;stopped → MUST transition to&#xa;another agent" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f3e5f5;strokeColor=#9673a6;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="850" y="265" width="210" height="125" as="geometry" />
        </mxCell>

        <mxCell id="romosozumab" value="Romosozumab (Evenity)&#xa;• Anti-sclerostin antibody&#xa;• DUAL action: ↑ formation&#xa;  + ↓ resorption&#xa;&#xa;BBW: MI/stroke/CV death risk&#xa;CI: MI or stroke within 1 year" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f3e5f5;strokeColor=#9673a6;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="850" y="405" width="210" height="130" as="geometry" />
