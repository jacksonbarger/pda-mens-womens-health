        <!-- Combined OCPs -->
        <mxCell id="coc-box" value="COMBINED ORAL CONTRACEPTIVES (COCs)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="1140" y="210" width="190" height="40" as="geometry" />
