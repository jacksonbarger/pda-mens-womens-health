        <mxCell id="zoledronic" value="Zoledronic Acid (Reclast)&#xa;• IV annually&#xa;• Most potent bisphosphonate&#xa;• NO GI side effects" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f3e5f5;strokeColor=#9673a6;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="600" y="340" width="220" height="75" as="geometry" />
        </mxCell>

        <mxCell id="bpp-admin" value="Oral Bisphosphonate Admin:&#xa;• Take FIRST THING in morning&#xa;• 6-8 oz water, 30-60 min BEFORE food&#xa;• Remain UPRIGHT 30-60 min&#xa;&#xa;CI: Hypocalcemia, CrCl <30-35" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;fontSize=11;align=left;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="600" y="430" width="220" height="130" as="geometry" />
        </mxCell>

        <mxCell id="bpp-ae" value="WARNING: BRONJ Risk&#xa;• Bisphosphonate-Related&#xa;  Osteonecrosis of Jaw&#xa;• Higher with IV > PO&#xa;• Dental surgery risk&#xa;&#xa;Drug Holiday: After 3-5 years" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;fontSize=11;align=left;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="600" y="575" width="220" height="130" as="geometry" />
