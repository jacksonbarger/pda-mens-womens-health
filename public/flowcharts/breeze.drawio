        <mxCell id="alendronate" value="Alendronate (Fosamax)&#xa;• PO weekly&#xa;• First-line oral therapy" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f3e5f5;strokeColor=#9673a6;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="600" y="265" width="220" height="60" as="geometry" />
