        <mxCell id="estrogen-class" value="Classification: C18 Steroid&#xa;• AROMATIC A-ring (essential)&#xa;• 3-Phenolic OH (essential)&#xa;• 17β-OH for maximal potency&#xa;&#xa;Classic estrogens:&#xa;• Estrone (E1)&#xa;• 17β-Estradiol (E2 - most potent)&#xa;• Estriol (E3)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="1140" y="265" width="190" height="170" as="geometry" />
        </mxCell>

        <mxCell id="ee-sar" value="Ethinyl Estradiol (EE)&#xa;• 17α-ETHINYL blocks metabolism&#xa;• Prevents estrone conversion&#xa;• 15-20x more potent orally&#xa;• Used in ALL oral contraceptives&#xa;&#xa;Mestranol:&#xa;• 3-O-methyl PRODRUG of EE" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
