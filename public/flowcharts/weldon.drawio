        <mxCell id="tamsulosin-sar" value="Tamsulosin (Flomax)&#xa;• Selective for alpha-1A&#xa;  (prostate > vasculature)&#xa;• LESS orthostatic hypotension&#xa;• Sulfonamide + benzene ring" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="60" y="410" width="220" height="100" as="geometry" />
        </mxCell>

        <mxCell id="doxazosin-sar" value="Doxazosin (Cardura)&#xa;• NON-selective alpha-1&#xa;• Quinazoline core&#xa;• Used for BPH + HTN&#xa;• MORE hypotension risk" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b3d9ff;strokeColor=#6c8ebf;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="60" y="525" width="220" height="95" as="geometry" />
