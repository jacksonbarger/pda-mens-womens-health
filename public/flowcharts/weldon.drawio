        <mxCell id="alpha-structure" value="Core Structure: QUINAZOLINE&#xa;Suffix: -AZOSIN&#xa;&#xa;Key Groups:&#xa;• Sulfonamide (essential)&#xa;• Piperazine/piperidine ring&#xa;• Methoxy substituents" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
