        <mxCell id="pearl6" value="Progesterone:&#xa;t1/2 5-10 min&#xa;→ synthetic progestins" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
