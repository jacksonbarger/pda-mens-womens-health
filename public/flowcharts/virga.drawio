        <mxCell id="pearl5" value="AI: Azole binds&#xa;heme iron of CYP19&#xa;→ blocks estrogen" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
