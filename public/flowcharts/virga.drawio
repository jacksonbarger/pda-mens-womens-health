        <mxCell id="pearl3" value="Clomiphene: E-isomer&#xa;= antiestrogenic,&#xa;Z-isomer = estrogenic" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
