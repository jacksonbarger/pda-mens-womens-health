        <mxCell id="pearl1" value="Aromatic A-ring +&#xa;3-phenolic OH =&#xa;ESSENTIAL for estrogen" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
