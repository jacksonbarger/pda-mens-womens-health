        <mxCell id="pearl7" value="C6 methyl in&#xa;pregnanes → oral&#xa;availability" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
          <mxGeometry x="1040" y="1210" width="140" height="70" as="geometry" />
