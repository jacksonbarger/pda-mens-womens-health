        <!-- TITLE -->
        <mxCell id="title" value="MEDICINAL CHEMISTRY - Dr. Virga&#xa;Gonadal Hormones: Estrogens | Progestins | Androgens | SERMs | SAR Principles" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;fontStyle=1;fontSize=18;align=center;verticalAlign=middle;" vertex="1" parent="1">
          <mxGeometry x="40" y="20" width="1520" height="80" as="geometry" />
        </mxCell>

        <!-- ========== SECTION 1: ESTROGENS ========== -->
        <mxCell id="estrogen-section" value="ESTROGEN CHEMISTRY & SAR" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;fontStyle=1;fontSize=16;" vertex="1" parent="1">
          <mxGeometry x="40" y="130" width="500" height="50" as="geometry" />
        </mxCell>

        <!-- Natural Estrogens -->
        <mxCell id="estrogen-class" value="CLASSIFICATION: C18 STEROID&#xa;&#xa;Key Features:&#xa;• AROMATIC A-ring (ESSENTIAL)&#xa;• 3-Phenolic OH (ESSENTIAL)&#xa;• 17β-OH for maximal potency&#xa;• 17α-OH → ↓↓ activity&#xa;&#xa;Aromatase (CYP19):&#xa;Converts androgens → estrogens&#xa;(requires 3 O2 + NADPH)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="60" y="210" width="220" height="190" as="geometry" />
        </mxCell>

        <mxCell id="classic-estrogens" value="3 CLASSIC ESTROGENS:&#xa;&#xa;Estrone (E1)&#xa;• Less potent&#xa;&#xa;17β-Estradiol (E2)&#xa;• MOST POTENT&#xa;• Rapid metabolism&#xa;&#xa;Estriol (E3)&#xa;• Weakest&#xa;&#xa;E1 ↔ E2 interconversion&#xa;(estradiol dehydrogenase)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffcccc;strokeColor=#b85450;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="60" y="415" width="220" height="195" as="geometry" />
        </mxCell>

        <!-- Synthetic Estrogens -->
        <mxCell id="synthetic-box" value="SYNTHETIC ESTROGENS" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="310" y="210" width="210" height="40" as="geometry" />
        </mxCell>

        <mxCell id="ee-sar" value="Ethinyl Estradiol (EE)&#xa;&#xa;17α-ETHINYL GROUP:&#xa;• Blocks oxidation of 17β-OH&#xa;• Prevents estrone conversion&#xa;• 15-20x more potent ORALLY&#xa;• 40% bioavailability&#xa;&#xa;Used in ALL oral contraceptives&#xa;&#xa;Mestranol:&#xa;• 3-O-methyl PRODRUG of EE&#xa;• Demethylated in liver" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="310" y="265" width="210" height="210" as="geometry" />
        </mxCell>

        <mxCell id="estradiol-esters" value="17β-Esters (IM Depot):&#xa;• Estradiol valerate&#xa;• Estradiol cypionate&#xa;• Long-acting (14-28 days)&#xa;• Esterases release estradiol&#xa;&#xa;Conjugated Estrogens (Premarin):&#xa;• From pregnant MARE urine&#xa;• Contains equilin/equilenin&#xa;  (unique to horses, B-ring&#xa;  unsaturated)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="310" y="490" width="210" height="180" as="geometry" />
        </mxCell>

        <!-- Estrogen Metabolism -->
        <mxCell id="estrogen-metab" value="ESTROGEN METABOLISM:&#xa;&#xa;Estradiol →&#xa;HYDROXYLATION (2/4-positions)&#xa;→ Catechol estrogens&#xa;→ COMT (methylation)&#xa;→ O-methoxy metabolites&#xa;→ CONJUGATION (glucuronide/sulfate)&#xa;→ EXCRETION&#xa;&#xa;Poor oral BA:&#xa;• Rapid 17β-OH oxidation&#xa;• Extensive conjugation" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="60" y="625" width="220" height="205" as="geometry" />
        </mxCell>

        <!-- Nonsteroidal Estrogens -->
        <mxCell id="nonsteroidal-box" value="NONSTEROIDAL ESTROGENS&#xa;(Stilbene Derivatives)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="60" y="850" width="220" height="50" as="geometry" />
        </mxCell>

        <mxCell id="des-sar" value="Diethylstilbestrol (DES)&#xa;&#xa;SAR:&#xa;• TRANS isomer MORE potent&#xa;• Two phenolic OH groups&#xa;  (mimic estradiol)&#xa;• Non-steroidal scaffold&#xa;• High oral activity&#xa;&#xa;Clinical Issues:&#xa;• Vaginal clear cell adenocarcinoma&#xa;  in offspring&#xa;• Uterine cancer risk&#xa;• NO LONGER USED in pregnancy" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="60" y="915" width="220" height="195" as="geometry" />
        </mxCell>

        <!-- ========== SECTION 2: ESTROGEN ANTAGONISTS & SERMs ========== -->
        <mxCell id="serm-section" value="ESTROGEN ANTAGONISTS & SERMs" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;fontStyle=1;fontSize=16;" vertex="1" parent="1">
          <mxGeometry x="580" y="130" width="500" height="50" as="geometry" />
        </mxCell>

        <!-- Clomiphene -->
        <mxCell id="clomiphene-box" value="CLOMIPHENE (Clomid)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="600" y="210" width="220" height="40" as="geometry" />
        </mxCell>

        <mxCell id="clomiphene-sar" value="Stereochemistry:&#xa;&#xa;E-isomer (Enclomiphene):&#xa;• ANTIESTROGENIC&#xa;&#xa;Z-isomer (Zuclomiphene):&#xa;• ESTROGENIC&#xa;&#xa;Dosed as RACEMIC MIXTURE&#xa;= PARTIAL AGONIST&#xa;&#xa;Mechanism for Infertility:&#xa;• Blocks ER in hypothalamus&#xa;• Prevents negative feedback&#xa;• ↑ GnRH → ↑ FSH/LH&#xa;• Induces ovulation&#xa;&#xa;Long t1/2 (~5 days)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="600" y="265" width="220" height="255" as="geometry" />
        </mxCell>

        <!-- Tamoxifen -->
        <mxCell id="tamoxifen-box" value="TAMOXIFEN (SERM)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="850" y="210" width="210" height="40" as="geometry" />
        </mxCell>

        <mxCell id="tamoxifen-metab" value="METABOLISM (Critical!):&#xa;&#xa;Tamoxifen (Z-isomer)&#xa;↓ CYP3A4 (demethylation)&#xa;N-desmethyltamoxifen&#xa;↓ CYP2D6 (hydroxylation)&#xa;4-Hydroxytamoxifen&#xa;(ACTIVE METABOLITE - 100x potency)&#xa;&#xa;CYP2D6 polymorphisms:&#xa;• Poor metabolizers → ↓ efficacy&#xa;• CYP2D6 inhibitors → ↓ effect" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="850" y="265" width="210" height="220" as="geometry" />
        </mxCell>

        <mxCell id="tamoxifen-effects" value="TISSUE-SELECTIVE EFFECTS:&#xa;&#xa;Breast: ANTAGONIST&#xa;• Treats ER+ breast cancer&#xa;• 5 years adjuvant therapy&#xa;&#xa;Endometrium: AGONIST&#xa;• ↑ Endometrial cancer risk&#xa;• Monitor for bleeding&#xa;&#xa;Bone: AGONIST&#xa;• Prevents bone loss&#xa;&#xa;Lipids: AGONIST&#xa;• ↓ LDL cholesterol" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="850" y="500" width="210" height="210" as="geometry" />
        </mxCell>

        <!-- Raloxifene -->
        <mxCell id="raloxifene-box" value="RALOXIFENE (Evista)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="600" y="545" width="220" height="40" as="geometry" />
        </mxCell>

        <mxCell id="raloxifene-sar" value="Benzothiophene SERM&#xa;&#xa;TISSUE-SELECTIVE EFFECTS:&#xa;&#xa;Breast: ANTAGONIST&#xa;• Breast cancer prevention&#xa;&#xa;Uterus: ANTAGONIST&#xa;• NO endometrial effects&#xa;• ↓ Cancer risk vs tamoxifen&#xa;&#xa;Bone: AGONIST&#xa;• Treats postmenopausal&#xa;  osteoporosis&#xa;&#xa;Pharmacokinetics:&#xa;• Very low oral BA (~5%)&#xa;• Extensive first-pass glucuronidation&#xa;• Enterohepatic recirculation&#xa;&#xa;CI: History of VTE" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="600" y="600" width="220" height="280" as="geometry" />
        </mxCell>

        <!-- Aromatase Inhibitors -->
        <mxCell id="ai-box" value="AROMATASE INHIBITORS" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="850" y="735" width="210" height="40" as="geometry" />
        </mxCell>

        <mxCell id="ai-sar" value="MOA:&#xa;• N-4 AZOLE NITROGEN binds&#xa;  HEME IRON of CYP19&#xa;• Prevents O2 binding&#xa;• Blocks catalysis&#xa;• Inhibits androgen → estrogen&#xa;&#xa;Nonsteroidal (Reversible):&#xa;• Anastrozole (Arimidex)&#xa;• Letrozole (Femara)&#xa;• Triazole structure&#xa;&#xa;Steroidal (Irreversible):&#xa;• Exemestane (Aromasin)&#xa;&#xa;Clinical Use:&#xa;• Postmenopausal ER+ breast cancer&#xa;• First-line or after tamoxifen&#xa;• Reduce estradiol 70-80%" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="850" y="790" width="210" height="300" as="geometry" />
        </mxCell>

        <!-- ========== SECTION 3: PROGESTINS ========== -->
        <mxCell id="progestin-section" value="PROGESTIN CHEMISTRY & GENERATIONS" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;fontStyle=1;fontSize=16;" vertex="1" parent="1">
          <mxGeometry x="1120" y="130" width="440" height="50" as="geometry" />
        </mxCell>

        <!-- Natural Progesterone -->
        <mxCell id="progesterone-box" value="PROGESTERONE (Natural)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="1140" y="210" width="190" height="40" as="geometry" />
        </mxCell>

        <mxCell id="progesterone-sar" value="Classification: C21 STEROID&#xa;• 3-Keto-4,5-ene structure&#xa;&#xa;Limitations:&#xa;• RAPID metabolism (t1/2 5-10 min)&#xa;• Excreted as 5β-pregnanediol&#xa;  conjugate&#xa;• Poor oral bioavailability&#xa;&#xa;→ Necessitates SYNTHETIC&#xa;    PROGESTINS for clinical use" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="1140" y="265" width="190" height="190" as="geometry" />
        </mxCell>

        <!-- Progestin Classes -->
        <mxCell id="progestin-class-box" value="SYNTHETIC PROGESTIN CLASSES" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="1140" y="480" width="190" height="40" as="geometry" />
        </mxCell>

        <mxCell id="pregnanes" value="PREGNANES:&#xa;• C6 METHYL → oral availability&#xa;• Medroxyprogesterone (MPA)&#xa;• Megestrol&#xa;• No androgenic activity&#xa;&#xa;19-NORPREGNANES:&#xa;• Very selective PR agonists&#xa;• Nomegestrol, Trimegestone&#xa;• Minimal androgenic activity" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="1140" y="535" width="190" height="175" as="geometry" />
        </mxCell>

        <mxCell id="estranes" value="ESTRANES (1st Gen):&#xa;• 19-Nortestosterone derivatives&#xa;• 17α-ETHINYL → oral activity&#xa;• Norethindrone (NET)&#xa;• Some androgenic activity&#xa;• Metabolized to estrogenic cpds" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="1140" y="725" width="190" height="125" as="geometry" />
        </mxCell>

        <mxCell id="gonanes" value="GONANES (2nd Gen):&#xa;• 13-ETHYL group (vs 13-methyl)&#xa;• ↑↑ Progestational potency&#xa;• Levonorgestrel (LNG)&#xa;  - HIGHEST androgenic activity&#xa;• Desogestrel → Etonogestrel&#xa;  (active, used in NuvaRing/Nexplanon)&#xa;• Gestodene, Norgestimate&#xa;  - LOWER androgenic" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="1140" y="865" width="190" height="175" as="geometry" />
        </mxCell>

        <!-- Progestin Generations -->
        <mxCell id="gen3-box" value="3RD GENERATION" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="1360" y="210" width="180" height="40" as="geometry" />
        </mxCell>

        <mxCell id="drospirenone" value="Drospirenone (Yasmin/Yaz)&#xa;&#xa;Derived from SPIRONOLACTONE&#xa;&#xa;UNIQUE Properties:&#xa;• ANTI-mineralocorticoid&#xa;  (diuretic effect)&#xa;• ANTI-androgenic&#xa;  (↓ acne, hirsutism)&#xa;• No estrogenic activity&#xa;• No glucocorticoid activity&#xa;&#xa;Clinical Use:&#xa;• COCs for women with PMDD, acne&#xa;• ↓ Water retention&#xa;&#xa;Caution: Monitor K+ levels" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="1360" y="265" width="180" height="235" as="geometry" />
        </mxCell>

        <!-- 4th Gen & Antagonists -->
        <mxCell id="gen4-box" value="4TH GENERATION" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="1360" y="525" width="180" height="40" as="geometry" />
        </mxCell>

        <mxCell id="dienogest" value="Dienogest&#xa;• Extremely selective PR agonist&#xa;• No androgenic activity&#xa;• Used for endometriosis&#xa;&#xa;Nestorone&#xa;• Ultra-selective PR&#xa;• Investigational (vaginal ring)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;" vertex="1" parent="1">
          <mxGeometry x="1360" y="580" width="180" height="140" as="geometry" />
        </mxCell>

        <!-- Progesterone Antagonists -->
        <mxCell id="antagonist-box" value="PROGESTERONE ANTAGONISTS" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontStyle=1;fontSize=13;" vertex="1" parent="1">
          <mxGeometry x="1360" y="745" width="180" height="40" as="geometry" />
        </mxCell>

        <mxCell id="mifepristone" value="Mifepristone (RU-486)&#xa;• Progesterone ANTAGONIST&#xa;• Also binds GR (glucocorticoid)&#xa;• Long half-life&#xa;• Medical abortion (+ misoprostol)&#xa;&#xa;Ulipristal Acetate (Ella)&#xa;• SPRM (Selective PR Modulator)&#xa;• Partial agonist/antagonist&#xa;• Emergency contraception&#xa;• Within 120 HOURS (5 days)&#xa;• More effective than LNG at 72-120h&#xa;• ONLY EC that delays ovulation" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;fontSize=11;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="1360" y="800" width="180" height="240" as="geometry" />
        </mxCell>

        <!-- KEY SAR PRINCIPLE -->
        <mxCell id="key-sar" value="KEY SAR PRINCIPLE:&#xa;&#xa;ETHINYL on ANDROGEN&#xa;↓&#xa;Converts to PROGESTIN&#xa;&#xa;19-NOR Modification:&#xa;Loss of C19 METHYL&#xa;↓&#xa;DECREASES androgenic activity&#xa;&#xa;This is how androgens are&#xa;converted to progestins used&#xa;in oral contraceptives!" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;fontSize=11;align=left;fontStyle=1;" vertex="1" parent="1">
          <mxGeometry x="1140" y="1055" width="190" height="230" as="geometry" />
        </mxCell>

        <!-- ========== HIGH-YIELD SAR PEARLS ========== -->
