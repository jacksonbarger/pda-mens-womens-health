        <mxCell id="pearl8" value="19-Nor: Loss C19&#xa;methyl → ↓ androgenic&#xa;activity" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
