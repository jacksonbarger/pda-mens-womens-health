        <mxCell id="pearl4" value="Tamoxifen prodrug:&#xa;CYP2D6 → 4-OH&#xa;(active metabolite)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=11;fontStyle=1;align=center;" vertex="1" parent="1">
